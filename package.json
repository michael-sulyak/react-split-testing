{
  "name": "react-split-testing",
  "version": "1.1.1",
  "description": "Simple A/B testing component for React.",
  "author": "expert-m",
  "license": "MIT",
  "repository": "expert-m/react-split-testing",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "scripts": {
    "test": "jest --env=jsdom --setupTestFrameworkScriptFile=raf/polyfill",
    "test:watch": "jest --watchAll --env=jsdom --setupTestFrameworkScriptFile=raf/polyfill",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "predeploy": "cd examples/simple && yarn install && yarn run build",
    "deploy": "gh-pages -d examples/simple/build"
  },
  "keywords": [
    "react",
    "ab",
    "a/b",
    "experiments",
    "testing",
    "ssr"
  ],
  "peerDependencies": {
